.card{
    min-height: 175px;
    padding: 0;
    text-align:center;
    border-radius: 10px;
    border: 1px solid transparent;
    // box-shadow: 0 7px 30px -10px rgba(150, 170, 180, 0.5);
    transition: all ease-in .2s;
    @media (min-width:600px){
        min-height: 125px; 
    }
    @media (min-width:900px) {
        min-height: 115px;  
    }
    @media (min-width:1200px){
        min-height: 135px; 
    }
    @media (min-width:1366px){
        min-height: 145px; 
    }
    &:hover{
        box-shadow: none;
    transition: all ease-in .2s;
    }
    a{
        font-size: 14pt;
        font-family: "Open Sans", sans-serif;
        text-decoration: none;
        font-weight: 400;
            display: grid;
            color:#000;
            @media (min-width:600px){
                font-size: 11pt;  
            }
            @media (min-width:900px) {
                font-size: 11pt;  
            }
            @media (min-width:1200px){
                font-size: 12pt;
            }
            @media (min-width:1366px){
                font-size: 14pt; 
            }

        &::before{
            margin-bottom: 10px;
            font-family: "Font Awesome 5 Pro";
            font-size: 16pt;
            padding: 16px 20px 15px 15px;
            border-radius: 50%;
            // max-width: 15%;
            margin-left: auto;
            margin-right: auto;
            margin-top: 10%;
            height: 50px;
            width: 50px;
            @media (min-width:600px){
                height: 40px;
                width: 40px;  
                    font-size: 12pt;
    padding: 13px 13px 9px 11px !important; 
            }
            @media (min-width:900px) {
                height: 40px;
            width: 40px;  
                font-size: 12pt;
padding: 13px 13px 9px 11px !important; 
            }
            @media (min-width:1200px){
                height: 40px;
                width: 40px;  
                    font-size: 12pt;
    padding: 13px 13px 9px 11px !important; 
            }
            @media (min-width:1366px){
                font-size: 16pt;
                height: 50px;
                width: 50px; 
                padding: 16px 20px 15px 15px !important;
                
            }

        }
    }


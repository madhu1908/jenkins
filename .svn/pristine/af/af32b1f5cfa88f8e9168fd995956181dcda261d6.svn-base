.pay-check input[type=checkbox]:before {
    width: 20px;
    height: 20px;
    margin-top: -2px;
    margin-left: -2px;
    content: "";
    border: 1px solid #c0c0c0;
    border-radius: 4px;
    background-color: transparent;
}
.pay-check input[type=checkbox] {
    position: relative !important;
    width: 100%;
    height: 100%;
    border-radius: 50%;
    margin: 0 !important;
    cursor: pointer;
    -webkit-user-select: none;
    user-select: none;
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
}
input[type=checkbox]:after {
    width: 8px;
    height: 4px;
    margin: 5px 4px;
    transform: rotate(-45deg);
    pointer-events: none;
    border: 2px solid #8c8c8c;
    border-top: 0;
    border-right: 0;
}
